{
  "name": "hurdle",
  "version": "1.0.0",
  "description": "A challenging word-guessing game based on Wordle",
  "main": "src/main.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:selenium": "node tests/selenium/run-selenium-tests.js",
    "test:selenium:headless": "HEADLESS=true node tests/selenium/run-selenium-tests.js",
    "test:e2e": "npm run test:selenium",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "start": "node server.js",
    "start:dev": "webpack serve --mode development --open",
    "docker:dev": "docker-compose up --build",
    "docker:dev:detached": "docker-compose up -d --build",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "db:setup": "docker-compose up -d mysql",
    "db:reset": "docker-compose down -v && docker-compose up -d mysql"
  },
  "keywords": [
    "wordle",
    "game",
    "word-game"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@vue/compiler-sfc": "^3.5.25",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "babel-loader": "^9.1.3",
    "chromedriver": "^143.0.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "fast-check": "^3.13.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "selenium-webdriver": "^4.39.0",
    "style-loader": "^3.3.3",
    "vue-loader": "^17.4.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "vue": "^3.5.25",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}
